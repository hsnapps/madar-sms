{
    "name": "laravel-notification-channels/madar-sms",
    "description": "This package makes it easy to send notifications using MadarSMS(https://mobile.net.sa/) with Laravel.",
    "homepage": "https://github.com/laravel-notification-channels/madar-sms",
    "license": "MIT",
    "authors": [
        {
            "name": "Hassan Ba Abdullah",
            "email": "prog.hasan@gmail.com",
            "homepage": "https://github.com/hsnapps",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^6.5 || ^7.0",
        "illuminate/notifications": "~6.0 || ~7.0 || ~8.0",
        "illuminate/support": "~6.0 || ~7.0 || ~8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Madar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Madar\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    }
}
